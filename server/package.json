{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/server.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "start": "dotenv -e src/.env -- npx node build/server.js",
    "build": "dotenv -e src/.env -- tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "clean": "dotenv -e src/.env -- tsc --build --clean",
    "dev": "dotenv -e src/.env -- npx ts-node src/server.ts",
    "migrate:create": "dotenv -e src/.env -- npx prisma migrate dev --name init"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/jsonwebtoken": "^9.0.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "hyper-express": "^6.14.3",
    "hyper-express-session": "^1.1.5",
    "jsonwebtoken": "^9.0.2",
    "live-directory": "^3.0.3",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.10.0",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.2"
  },
  "_moduleAliases": {
    "@/utils": "src/utils",
    "@/middleware": "src/middleware",
    "@/resources": "src/resources"
  }
}
